body{
   background-color:white; 
   background-image: url(design.png);
   background-repeat: repeat-x;
   height: 100%;
   background-position: center;
   background-repeat: no-repeat;
   background-size: cover;
} 
}
.wrap{
    width: 40%;
    margin: auto;
    color: :-moz-only-whitespace;
    border-radius: 10px;
    font-family:verdana;
    background-color:white;
}
.container{
    padding: 5px;
}
table{
    margin: auto;
}
table td{
    padding: 5px;
}
button{
    width: 50px;
    font-family:verdana;
    height: 30px;
    background-color: white;
    background-radius: 10px;
}
ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
  }
  